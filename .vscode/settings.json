{
	"search.exclude": {
		"export/**/*.*": true
	},
	"[haxe]": {
		"editor.formatOnSave": true,
		"editor.formatOnSaveMode":"file",
		"editor.formatOnPaste": false,
		"editor.codeActionsOnSave": {
			"source.sortImports": "explicit"
		}
	},
	"haxe.enableExtendedIndentation": true,

	"files.associations": {
                "*.hxc": "haxe",
                "*.dream": "json"
	}
}
